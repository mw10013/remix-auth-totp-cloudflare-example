{
  "name": "remix-cf",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client --compatibility-date=2024-02-03 --compatibility-flags=nodejs_compat",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "tail:prod": "wrangler pages deployment tail --project-name remix-auth-totp-cloudflare-example --environment production",
    "tail:preview": "wrangler pages deployment tail --project-name remix-auth-totp-cloudflare-example --environment preview",
    "tail:deploy": "wrangler pages deployment tail --project-name remix-auth-totp-cloudflare-example",
    "tunnel": "cloudflared tunnel --url http://localhost:8788",
    "drizzle:generate": "drizzle-kit generate:sqlite",
    "drizzle:up": "drizzle-kit up:sqlite",
    "drizzle:check": "drizzle-kit check:sqlite",
    "drizzle:drop": "drizzle-kit drop",
    "d1:migrate:list": "wrangler d1 migrations list ratce-d1-dev --local",
    "d1:migrate:apply": "wrangler d1 migrations apply ratce-d1-dev --local",
    "d1:migrate:list:preview": "wrangler d1 migrations list ratce-d1-preview --env preview",
    "d1:migrate:apply:preview": "wrangler d1 migrations apply ratce-d1-preview --env preview",
    "d1:migrate:list:prod": "wrangler d1 migrations list ratce-d1-prod --env prod",
    "d1:migrate:apply:prod": "wrangler d1 migrations apply ratce-d1-prod --env prod",
    "d1:reset": "tsx scripts/d1-reset.ts",
    "d1:push": "tsx scripts/d1-push.ts",
    "script": "tsx --env-file=.dev.vars"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.0",
    "@conform-to/zod": "^1.0.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@remix-run/cloudflare": "^2.6.0",
    "@remix-run/cloudflare-pages": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.29.3",
    "isbot": "^4.1.0",
    "lucide-react": "^0.321.0",
    "miniflare": "^3.20231030.4",
    "nanoid": "^5.0.5",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-twc": "^1.4.1",
    "remix-auth": "^3.6.0",
    "remix-auth-totp": "github:mw10013/remix-auth-totp#session",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@remix-run/dev": "^2.6.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.17",
    "better-sqlite3": "^9.4.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "shadcn-ui": "^0.8.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.28.2",
    "zx": "^7.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
